				<arg2 name="minlength" key="label.nombre" />
				<var>
					<var-name>mask</var-name>
					<var-value>^[a-zA-Z]*$</var-value>
				</var>
				<var>
					<var-name>minlength</var-name>
					<var-value>2</var-value>
				</var>
			</field>
